    Inner = Path()
    {
        SvgData = svg_gripper_rhs;
        FillColor = colorBrightBorder;
    };
};




// Helper to rasterize an image
var makeImage = (size, svg, color) => Rasterize()
{
    Size = (size, size);
    Inner = Path()
    {
        SvgData = svg;
        FillColor = color;
    };
};

// Helper to rasterize an image
var makeImageWH = (width, height, svg, color) => Rasterize()
{
    Size = (width, height);
    Inner = Path()
    {
        SvgData = svg;
        FillColor = color;
    };
};

// Helper to rasterize a 2-color image
var makeImage2 = (size, svg, color, svg2, color2) => Rasterize()
{
    Size = (size, size);
    Inner = [
        Path()
        {
            SvgData = svg;
            FillColor = color;
        },
        Path()
        {
            SvgData = svg2;
            FillColor = color2;
        },
    ];
};


// Template class for glow buttons
class GlowButton<size, svg, color, colorPressed>
{
    Image = Frame()
    {
        InnerSize = (size, size);
        Gravity = Center;
        Inner =
        {
            (Inactive || Disabled) = makeImage(size, svg, colorDisabledText);
            Checked = makeGlowImage(size, svg, color);
            Selected = makeImage(size, svg, colorFocusText);
            (Focused || Pressed) = makeImage(size, svg, colorPressed);
            else = makeImage(size, svg, colorToolbarButton);
        };
    };
}


// Template class for glow buttons with alternate color
class GlowButton<size, svg, color, colorAlt, colorPressed>
{
    Image = Frame()
    {
        InnerSize = (size, size);
        Gravity = Center;
        Inner =
        {
            Inactive = makeImage(size, svg, colorDisabledText);
            Alternate = makeGlowImage(size, svg, colorAlt);
            Checked = makeGlowImage(size, svg, color);
            (Focused || Pressed) = makeImage(size, svg, colorPressed);
            else = makeImage(size, svg, colorToolbarButton);
        };
    };
}

// Helper to make a standard toolbar button
var makeToolbarButton = (size, pressed, svg, colorPressed) => Rasterize()
{
    Size = (size, size);
    Gravity = Center;
    Inner = [
        Path()
        {
            SvgData = svg;
            FillColor = pressed ? colorPressed : colorToolbarButton;
            MaintainAspectRatio = true;
        },
    ];
};

// Template class for standard toolbar button
class ButtonImage<size, svg, colorNormal, colorPressed>
{
    Image = Frame()
    {
        InnerSize = (size, size);
        Gravity = Center;
        Inner =
        {
            (Focused || Selected || Pressed) = makeToolbarButton(size, true, svg, colorPressed);
            else = makeToolbarButton(size, false, svg, colorNormal);
        };
    };
}


// A button with image with different colors for checked vs unchecked
class ButtonImage<size, svgNormal, colorNormal, svgChecked, colorChecked, colorPressed>
{
    Image = Frame()
    {
        InnerSize = (size, size);
        Gravity = Center;
        Inner =
        {
            Checked && (Pressed || Focused || Selected) = makeImage(size, svgChecked, colorPressed);
            Checked = makeImage(size, svgChecked, colorChecked);
            (Pressed || Focused || Selected) = makeImage(size, svgNormal, colorPressed);
            else = makeImage(size, svgNormal, colorNormal);
        };
    };
}

// A button with image with different colors for checked vs unchecked
class ButtonImageWH<width, height, svgNormal, colorNormal, svgChecked, colorChecked, colorPressed>
{
    Image = Frame()
    {
        InnerSize = (width, height);
        Gravity = Center;
        Inner =
        {
            Checked && (Pressed || Focused || Selected) = makeImageWH(width, height, svgChecked, colorPressed);
            Checked = makeImageWH(width, height, svgChecked, colorChecked);
            (Pressed || Focused || Selected) = makeImageWH(width, height, svgNormal, colorPressed);
            else = makeImageWH(width, height, svgNormal, colorNormal);
        };
    };
}


// The beat indicator in the main status panel
